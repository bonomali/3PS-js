{
    "extends": "standard",
    "env": {
        "es6": true,
        "node": true
    },
    "rules": {
        "indent": "off",
        "no-trailing-spaces": "off",
        "key-spacing": "off",
        "no-undef": "off",
        "no-console": "off",
        "padded-blocks": "off",
        "comma-dangle": "off",
        "no-multi-spaces": "off",
        "no-multiple-empty-lines": "off"
    },
    "root": true
}